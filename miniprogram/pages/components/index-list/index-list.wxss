/* components/index-list/index-list.wxss */

.main{
  height: 320rpx;
  overflow: hidden;
}
.main-img{
  width:100%;
  height: 100%;
}
.main-text{
  font-size: 24rpx;
  margin: 10rpx;
}
.li{
  background-color: #fff;
  margin-bottom: 10rpx;
  border-radius: 20rpx;
  width: 45%;
}
.main-span{
  font-size: 20rpx;
  color: #999;
  margin: 10rpx;
}
.main-loot{
  margin: auto;
  width: 90%;
  background-color: #ff5f1e;
  border-radius: 10rpx;
  height: 60rpx;
  margin-bottom: 10rpx;

}
.main-loot-right{
  float: right;
  line-height: 60rpx;
  margin-right: 20rpx;
  font-weight: 700;
  color:#692106 ;
  font-size: 35rpx;
}
.main-loot-left{
  float: left;
  color: #fff;
  line-height: 60rpx;
}
.span{
  font-size: 20rpx;
  color: #e5e5e5;
  text-decoration: line-through;
}
