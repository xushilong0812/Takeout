
page {
  background: #F3F3F3;
}
.box{
  background-color: #ffc300;
  height:250rpx;
}
.head{
  margin: 0 20rpx;
  display: flex;

}
.head-img{
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  background-color: #fff;

}
.head-img image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.head-text{
  margin: 20rpx;
}
.head-name{
  font-weight: 700;
  margin-top: 10rpx;
}
.head-text-span{
  width: 150rpx;
  height: 50rpx;
  border-radius: 10rpx;
  border:  1rpx solid #000;
font-size: 25rpx;
text-align: center;
line-height: 50rpx;
margin-top: 10rpx;
}

/* 服务 */
.myserve{
  margin: 30rpx 30rpx;
  background-color: #fff;
  border-radius: 15rpx;

}
.myserve-title{
padding: 20rpx 20rpx;
font-weight: 800;
}
.my-img{
  width: 60rpx;
  height: 60rpx;
}
.list{
  width: 25%;
  text-align: center;
  font-size: 30rpx;
}
.list-f{
  display: flex;
  padding-bottom: 40rpx;
}
/* 钱包 */
.wallet{
  margin: 30rpx 30rpx;
  background-color: #fff;
  border-radius: 15rpx;
}
.bill{
  display: flex;
  padding-bottom: 40rpx;
}
.bill-li{
  width: 33%;
  text-align: center;
}
.span{
  font-weight: 700;
  font-size: 40rpx;
  display:inline-block ;
}
.span-1{
  display:inline-block ;
  font-size: 20rpx;
  margin-left: 5rpx;

}
.bill-{
  margin-top: 10rpx;
  font-size: 24rpx;
}
.bill-grey{
  margin-top: 5rpx;
  font-size: 20rpx;
  color: #999;
}
.list-img{
  width: 60rpx;
  height: 60rpx;
}
.list-text{
  font-size: 24rpx;
}
/* 钱包  end*/


 